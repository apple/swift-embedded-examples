{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-Xfrontend", "-disable-stack-protector",
      "-enable-experimental-feature", "Embedded",
      "-Xcc", "-mcpu=cortex-m0plus"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-arch", "armv6m",
      "-dead_strip",
      "-static",
      "-e", "_reset",
      "-no_zero_fill_sections",
      "-segalign", "4",
      "-segaddr", "__RESET", "0x20000000",
      "-segaddr", "__VECTORS", "0x20000100",
      "-seg1addr", "0x20000200",
      "-pagezero_size", "0",
      "-allow_dead_duplicates"
    ]
  }
}
