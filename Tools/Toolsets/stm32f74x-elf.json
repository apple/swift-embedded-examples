{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-enable-experimental-feature", "Embedded",
      "-Xfrontend", "-disable-stack-protector",
      "-Xfrontend", "-function-sections",
      "-Xfrontend", "-mergeable-symbols",
      "-Xclang-linker", "-nostdlib"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-T", "Sources/Support/linkerscript.ld",
      "-static",
      "-e", "_start"
    ]
  }
}
